      "position": [460, 300],
      "credentials": {
        "dataStoneApi": {
          "id": "",
          "name": "Data Stone API"
        }
      }
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "field-nome",
              "name": "Nome",
              "value": "={{ $json.name || $json.nome }}",
              "type": "string"
            },
            {
              "id": "field-cpf",
              "name": "CPF",
              "value": "={{ $json.cpf }}",
              "type": "string"
            },
            {
              "id": "field-email",
              "name": "Email",
              "value": "={{ Array.isArray($json.emails) ? $json.emails.join(', ') : ($json.email || '') }}",
              "type": "string"
            },
            {
              "id": "field-telefone",
              "name": "Telefone",
              "value": "={{ $json.phone || $json.telefone }}",
              "type": "string"
            },
            {
              "id": "field-endereco",
              "name": "Endereço",
              "value": "={{ $json.address || $json.endereco }}",
              "type": "string"
            },
            {
              "id": "field-uf",
              "name": "UF",
              "value": "={{ $json.uf || $json.state }}",
              "type": "string"
            }
          ]
        },
        "options": {}
      },
      "id": "d4d4d4d4-0003-4000-8000-000000000003",
      "name": "Organizar Dados",
      "type": "n8n-nodes-base.set",
